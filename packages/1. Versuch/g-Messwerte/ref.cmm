#include <stdio.h>
#include <stdlib.h>

void main() {

	float f;
	float small = 0, big = 0, avg = 0;
	int i;

	for( i = 0; i < 30; i++ ) {
		f = atof(scanf());
		avg += f;
		if( i == 0 || f < small )
			small = f;
		if( i == 0 || f > big )
			big = f;
	}

	avg /= 30;

	printf("Kleinste: %f\nGroesste: %f\nDurchschnitt: %f", small, big, avg);
}
