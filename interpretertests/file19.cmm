/*
 * implementation of stdio.h of the c standard-lib
 *
 * http://www.cplusplus.com/reference/cstdio/
 */

//------------------- Forward declarations

void putc(char ch);
void prints(string s);

//------------------- declarations

// print character
void putc(char ch) {
    print(ch);
}

// print string
void prints(string s) {
    int i = 0; 
    while(i < length(s)) {
        putc(s[i]);
        i += 1;    
    }
}

void main(){
	string s = "hallo";

	prints(s);
}
